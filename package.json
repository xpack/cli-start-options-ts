{
  "name": "@xpack/cli-start-options",
  "version": "0.10.0-pre",
  "description": "Classes to implement a command line Node.js application",
  "type": "module",
  "exports": {
    "import": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile-watch": "tsc --build --verbose --watch src tests",
    "fix": "ts-standard --fix --project src/tsconfig.json src && ts-standard --fix --project tests/tsconfig.json tests",
    "compile": "tsc --build --verbose src tests",
    "npm-install": "npm install",
    "npm-link": "npm link",
    "npm-outdated": "npm outdated",
    "npm-update": "npm update",
    "npm-version-patch": "npm version patch",
    "npm-version-minor": "npm version minor",
    "postversion": "git push origin --all && git push origin --tags",
    "package": "npm pack",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "lint": "echo ts-standard src",
    "prepare": "npm run compile && npm run lint && npm run typedoc",
    "pretest": "npm run compile && npm run lint",
    "pretest-100": "npm run compile",
    "pretest-100-c8": "npm run compile",
    "tap": "tap --reporter=spec",
    "test-tap": "tap",
    "test-tap-coverage": "tap --coverage",
    "test-tap-coverage-c8": "c8 -- tap",
    "test-tap-coverage-100": "tap --coverage --100",
    "test-tap-coverage-100-c8": "c8 --100 -- tap",
    "test": "npm run test-tap -s",
    "test-100": "npm run test-tap-coverage-100 -s",
    "test-100-c8": "npm run test-tap-coverage-100-c8 -s",
    "typedoc": "typedoc",
    "deep-clean": "rm -rf dist tests-out .nyc_output node_modules package-lock.json",
    "npm-version": "echo $(which node) $(node --version) && echo $(which npm) $(npm --version)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpack/cli-start-options-js.git"
  },
  "keywords": [
    "cli",
    "options"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/cli-start-options-js/issues/"
  },
  "homepage": "https://github.com/xpack/cli-start-options-js/",
  "dependencies": {
    "del": "^7.0.0",
    "is-ci": "^3.0.0",
    "is-installed-globally": "^0.4.0",
    "is-path-inside": "^4.0.0",
    "latest-version": "^7.0.0",
    "make-dir": "^3.1.0",
    "semver": "^7.3.8",
    "semver-diff": "^4.0.0"
  },
  "devDependencies": {
    "@types/is-ci": "^3.0.0",
    "@types/node": "^14.18.36",
    "@types/tap": "^15.0.8",
    "c8": "^7.13.0",
    "minimist": "^1.2.7",
    "standard": "17.0.0",
    "tap": "^16.3.4",
    "tar": "^6.1.13",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": " >=14.13.1 || >=15.3.0 || >=16.0.0"
  }
}
